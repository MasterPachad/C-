#include <iostream>

using namespace std;

int main()
{
   int draw;
   int x;
   int y;
   int a;
   int b;
   int c;

   x = 0;
   y = 0;

   do {

   do {
   draw = rand() % 3 + 1;
   switch(draw) {
   case(1): {
       if (a != 1){
       cout << "Tower\n";
       a = 1;
       x++;
    }

   }
   case(2): {
       if (b != 1){
       cout << "The Devil\n";
       b = 1;
       x++;
    }

   }
   case(3): {
       if (c != 1) {
       cout << "The Sun\n";
       c = 1;
       x++;
    }

   }
   }
   } while (x < 3);

   a = 0;
   b = 0;
   c = 0;

   y++;
   } while (y < 7);


   return 0;

}
